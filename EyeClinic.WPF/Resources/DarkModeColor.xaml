<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="PrimaryColor" >#252736</Color>
    <Color x:Key="PrimaryColor1" >#1E1D2B</Color>
    <Color x:Key="PrimaryColor2" >#383B48</Color>
    <Color x:Key="PrimaryColor3" >#2D303E</Color>
    <Color x:Key="PrimaryColor4" >#2D354E</Color>
    <SolidColorBrush x:Key="PrimaryColorBrush" Color="{StaticResource PrimaryColor}" />
    <SolidColorBrush x:Key="PrimaryColorBrush1" Color="{StaticResource PrimaryColor1}" />
    <SolidColorBrush x:Key="PrimaryColorBrush2" Color="{StaticResource PrimaryColor2}" />
    <SolidColorBrush x:Key="PrimaryColorBrush3" Color="{StaticResource PrimaryColor3}" />
    <SolidColorBrush x:Key="PrimaryColorBrush4" Color="{StaticResource PrimaryColor4}" />
</ResourceDictionary>